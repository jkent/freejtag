{
    "configurations": [
        {
            "name": "AVR",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/third-party/lufa"
            ],
            "defines": [
                "__AVR_ATmega16U2__",
                "USE_LUFA_CONFIG_HEADER",
                "F_CPU=8000000",
                "__uint24=uint32_t"
            ],
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}